{
  "rewrites": [
    {
      "source": "/((?!static|assets|images|favicon|manifest|robots|sitemap).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}