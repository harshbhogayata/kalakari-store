{
  "name": "kalakari-shop",
  "version": "1.0.0",
  "description": "Premier e-commerce platform for authentic Indian handcrafted products",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "start": "cd server && npm start"
  },
  "keywords": [
    "e-commerce",
    "artisan",
    "handcrafted",
    "india",
    "react",
    "nodejs"
  ],
  "author": "Kalakari.shop Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2"
  }
}
